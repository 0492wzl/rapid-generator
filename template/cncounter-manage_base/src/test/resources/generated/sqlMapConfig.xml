<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="defaultStatementTimeout" value="25000" />
	</settings>

	<typeAliases>
		<typeAlias type="com.ybs.exam.entity.po.QuestionCategory" alias="QuestionCategory" />
		<typeAlias type="com.ybs.exam.entity.po.MedicalSubject" alias="MedicalSubject" />
		<typeAlias type="com.ybs.exam.entity.po.TrainingCourse" alias="TrainingCourse" />
		<typeAlias type="com.ybs.exam.entity.po.SourceQuestion" alias="SourceQuestion" />
		<typeAlias type="com.ybs.exam.entity.po.OrgQuestion" alias="OrgQuestion" />
		<typeAlias type="com.ybs.exam.entity.po.QuestionOption" alias="QuestionOption" />
		<typeAlias type="com.ybs.exam.entity.po.ProjectQuestion" alias="ProjectQuestion" />
		<typeAlias type="com.ybs.exam.entity.po.CourseCategory2QuestionCategoryMapping" alias="CourseCategory2QuestionCategoryMapping" />
	</typeAliases>
	
</configuration>